{
  "rims": {
    "fileColumns": [
      {
        "name": "code",
        "pattern": "/^[0-9]{5}$/",
        "type": "string",
        "size": 5,
        "order": 1,
        "schemaConfig": {
          "required": true
        }
      },
      {
        "name": "width",
        "pattern": "/^\\d{1,2}.\\d{2}$/",
        "type": "number",
        "size": 5,
        "order": 2
      },
      {
        "name": "height",
        "pattern": "/^[A-Z]{1}$/",
        "type": "string",
        "size": 1,
        "order": 3
      },
      {
        "name": "diameter",
        "pattern": "/^\\d{2}$/",
        "type": "number",
        "size": 2,
        "order": 5
      },
      {
        "name": "one_piece",
        "pattern": "/^[xX]{1}$/",
        "type": "boolean",
        "config": {
          "true": "X",
          "false": "x"
        },
        "size": 1,
        "order": 4
      },
      {
        "name": "material",
        "pattern": "/^[SL]{1}$/",
        "type": "string",
        "size": 1,
        "order": 6
      }
    ]
  }
}
